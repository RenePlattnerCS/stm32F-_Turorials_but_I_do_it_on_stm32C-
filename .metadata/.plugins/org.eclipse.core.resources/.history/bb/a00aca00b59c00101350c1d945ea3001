#include "stm32C0xx.h"

#define ADC1EN				(1U << 20)
#define APB_CLOCK_EN		(1U << 10)
#define GPIOAEN				(1U<<0)

void pa1_adc_init(void)
{
	//Enable clock to GPIO pin for ADC chanel 1
	RCC->IOPENR |= GPIOAEN;
	//set mode to analog
	GPIOA->MODER |= (1U<<2);
	GPIOA->MODER |= (1U<<3);

	//configure ADC
	//enable clock to ADC

	//config adc paameters
}
